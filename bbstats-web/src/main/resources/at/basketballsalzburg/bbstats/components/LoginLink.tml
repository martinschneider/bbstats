<t:container xmlns="http://www.w3.org/1999/xhtml"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">

	<t:security.notAuthenticated>
		<t:actionlink t:id="tynamoLoginLink" id="tynamoLoginLink">${message:login}</t:actionlink>
	</t:security.notAuthenticated>

	<t:security.authenticated>
		<t:actionlink t:id="tynamoLogoutLink" id="tynamoLogoutLink">${message:logout}</t:actionlink>
	</t:security.authenticated>

</t:container>